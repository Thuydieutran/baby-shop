{% extends "base.html.twig" %}
{% form_theme form 'bootstrap_4_layout.html.twig' %}

{% block body %}

<div class="container mt-5">
        <h1 class="mb-5">Add a new Program</h1>
    {{ form_errors(form) }}
    {{ form_start(form, {'attr': {'novalidate': 'novalidate'}}) }}
    {{ form_widget(form) }}

    <input class="btn bg-success text-light my-5" type='submit' value='Add program'>

    {{ form_end(form) }}
</div>
{% endblock %}